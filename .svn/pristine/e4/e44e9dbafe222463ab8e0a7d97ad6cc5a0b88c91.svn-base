$color1:#22bf98;  //默认风格
$color2:#2bbabf;
$color3:#16aeef;
$color4:#f75555;
$color5:#fa6543;
body{
  //组件激活状态
  .active{
    background:$color1;
  }
  //侧边栏颜色
  .sidebar-wrapper{
    .el-submenu{
      .el-submenu__title{
        border-bottom:1px solid #00b889;
        background: $color1;
        color: #fff;
        &:hover{
          background: $color1 !important;
          color: #fff !important;
        }
      }
      &:hover{
        background: $color1 !important;
        color: #fff !important;
      }
    }
    background:$color1;
    .el-menu-item {
    background: $color1;
    color: #fff;
    border-bottom:1px solid #00b889;
    &:hover{
      background: $color1;
      color: #fff;
    }
    &.is-active{
      background-color:#05ac84  !important;
      color: #fff;
    }
  }
  }
  //顶部导航栏样式调整
  .navbar{
    .el-menu{
      background:#1f2c33;
      .el-menu-item{
        color:#abbac1;
        border-bottom:5px solid transparent !important;
        &:hover{
          background:#1f2c33;
          color:$color1;
          border-bottom:5px solid transparent !important;
        }
        &.is-active{
          color:$color1;
        }

      }
    }
  }

  //单选框高亮
  .el-radio__input.is-checked .el-radio__inner {
    border-color: $color1 !important;
    background: $color1 !important;
  }
  .el-checkbox__input.is-checked .el-checkbox__inner {
    border-color: $color1 !important;
    background: $color1 !important;
  }
  //主要按钮的颜色
  .el-button--primary {
    color: #fff;
    background-color: $color1;
    border-color: $color1;
    &:hover{
      background-color: $color1;
      border-color: $color1;
    }
  }
}


